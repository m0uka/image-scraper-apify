{
  "title": "Image Scraper",
  "description": "Input settings",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "startUrls": {
      "title": "Start URLs",
      "type": "array",
      "description": "URLs to start with.",
      "editor": "requestListSources",
      "prefill": [
        {
          "url": "https://apify.com"
        }
